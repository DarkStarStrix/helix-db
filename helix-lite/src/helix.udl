namespace helix_lite {
    
};

[Error]
enum HelixLiteError {
    "Default"
};

[Enum]
interface Input {
    String(string);
    Integer(i32);
    Float(f64);
};

interface HelixEmbedded {
    [Throws=HelixLiteError]
    string query(string query, sequence<Input> input);
    [Throws=HelixLiteError]
    constructor(string user_path);
};